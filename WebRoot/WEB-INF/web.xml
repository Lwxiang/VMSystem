<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <display-name></display-name>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>SER</servlet-name>
    <servlet-class>com.test.servlet.SER</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.ll.vmsystem.serlvet.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>ManagerServlet</servlet-name>
    <servlet-class>com.ll.vmsystem.serlvet.ManagerServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>MapServlet</servlet-name>
    <servlet-class>com.ll.vmsystem.serlvet.MapServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>DriverServlet</servlet-name>
    <servlet-class>com.ll.vmsystem.serlvet.DriverServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>FreightServlet</servlet-name>
    <servlet-class>com.ll.vmsystem.serlvet.FreightServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>GuardServlet</servlet-name>
    <servlet-class>com.ll.vmsystem.serlvet.GuardServlet</servlet-class>
  </servlet>







  <servlet-mapping>
    <servlet-name>SER</servlet-name>
    <url-pattern>/servlet/SER</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/servlet/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ManagerServlet</servlet-name>
    <url-pattern>/servlet/ManagerServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>MapServlet</servlet-name>
    <url-pattern>/servlet/MapServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DriverServlet</servlet-name>
    <url-pattern>/servlet/DriverServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>FreightServlet</servlet-name>
    <url-pattern>/servlet/FreightServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GuardServlet</servlet-name>
    <url-pattern>/servlet/GuardServlet</url-pattern>
  </servlet-mapping>	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
